MainHaxGui.Visible = false
MainHaxGui.Active = true
MainHaxGui.Draggable = true

SazaHub.Name = "SazaHub"
SazaHub.Parent = MainHaxGui
SazaHub.BackgroundColor3 = Color3.fromRGB(0, 0, 163)
