LoginButton.Name = "LoginButton"
LoginButton.Parent = LoginFrame
LoginButton.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
LoginButton.Position = UDim2.new(0.225531906, 0, 0.808118045, 0)
LoginButton.Size = UDim2.new(0, 127, 0, 35)
LoginButton.Font = Enum.Font.SourceSans
LoginButton.Text = "Login"
LoginButton.TextColor3 = Color3.fromRGB(0, 0, 0)
LoginButton.TextScaled = true
LoginButton.TextSize = 14.000
LoginButton.TextWrapped = true
LoginButton.MouseButton1Down:Connect(function()
	if Login.Text == "Reflex2323" and Password.Text == "DontBeShy" then
		LoginFrame.Visible = false
		MainHaxGui.Visible = true
	end
end)

MainHaxGui.Name = "MainHaxGui"
MainHaxGui.Parent = MainGui
MainHaxGui.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
