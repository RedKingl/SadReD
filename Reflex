Login.TextWrapped = true

Password.Name = "Password"
Password.Parent = LoginFrame
Password.BackgroundColor3 = Color3.fromRGB(94, 94, 94)
Password.Position = UDim2.new(0.144680858, 0, 0.55350554, 0)
Password.Size = UDim2.new(0, 168, 0, 50)
Password.Font = Enum.Font.SourceSansBold
Password.Text = "Password"
Password.TextColor3 = Color3.fromRGB(0, 0, 0)
